<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.charlie.mpandroidcharttest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".chartactivity.BarChartActivity" />
        <activity android:name=".chartactivity.TwoBarChartActivity" />
        <activity android:name=".chartactivity.ThreeBarChartActivity" />
        <activity android:name=".chartactivity.PositiveNegativeBarChartActivity" />
        <activity android:name=".chartactivity.LineChartActivity" />
        <activity android:name=".chartactivity.MultiLineChartActivity" />
        <activity android:name=".chartactivity.PieChartActivity" />
        <activity android:name=".chartactivity.CombineChartActivity"></activity>
    </application>

</manifest>